{
  "scripts": {
    "dev": "ts-node index",
    "start": "cd ./.ts && cross-env NODE_ENV=production node index",
    "build": "bash ./scripts/build.sh",
    "build-win32": "rmdir /Q /S dist & mkdir dist & npm run build:client & npm run build:server",
    "build:test": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.ts --progress --hide-modules && cross-env NODE_ENV=production webpack --config build/webpack.client.config.ts --progress --hide-modules",
    "lint": "eslint $(find . \\( -name \"*.js\" -o -name \"*.ts\" -o -name \"*.vue\" \\) -not \\( -path \"./node_modules/*\" -o -path \"./dist/*\" -o -name \".*\" -o -path \"./.ts/*\" \\))",
    "lint-fix": "eslint --fix $(find . \\( -name \"*.js\" -o -name \"*.vue\" \\) -not \\( -path \"./node_modules/*\" -o -path \"./dist/*\" -o -name \".*\" -o -path \"./.ts/*\" \\))",
    "test": "npm run build:test && npm run test:run",
    "test:run": "cross-env NODE_ENV=test nyc mocha ./test/**/*.test.ts --recursive --timeout 999999 --colors --exit --require ts-node/register --require source-map-support/register --full-trace",
    "cover": "nyc report --reporter=text-lcov | coveralls && rm -rf ./coverage && rm -rf ./.nyc_output",
    "upgrade": "node scripts/upgrade.js"
  },
  "nyc": {
    "require": [
      "ts-node/register",
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true,
    "extension": [
      ".ts",
      ".js"
    ]
  },
  "dependencies": {
    "acorn": "^7.2.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bufferutil": "^4.0.1",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "highlight.js": "^10.0.3",
    "markdown-it": "^11.0.0",
    "memory-fs": "^0.5.0",
    "mongodb": "^3.5.7",
    "multer": "^1.4.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "regenerator-runtime": "^0.13.5",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ts-node": "^8.9.1",
    "ua-parser-js": "^0.7.21",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.11",
